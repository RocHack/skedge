BEGIN:VCALENDAR
VERSION:2.0
<% @schedule.sections.each do |section| %>
BEGIN:VEVENT
CLASS:PUBLIC
DESCRIPTION:<%== section.course.title %>
DTSTART;TZID=America/New_York:<%== next_start_date(section) %>
DTEND;TZID=America/New_York:<%== next_end_date(section) %>
LOCATION:<%== section.decorate.place %>
SUMMARY;LANGUAGE=en-us:<%== section.course.department.short + " " + section.course.number %>
TRANSP:TRANSPARENT
RRULE:<%== recurrence_rule(section) %>;WKST=SU
END:VEVENT
<% end %>
END:VCALENDAR